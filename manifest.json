{
  "manifest_version": 2,
  "name": "Sidemarking",
  "description": "Rather than keep open dozens of tabs, save them with sidemarking",
  "version": "0.1",
  "browser_action": {
    "default_icon": "bookmark-icon-64x64.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "tabs"
  ]
}
